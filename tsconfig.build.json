{
	"compilerOptions": {
		"baseUrl": "./src",
		"paths": {
			".": ["."]
		},
		"target": "ES2020",
		"module": "esnext",
		"declaration": false,
		"outDir": "dist",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"allowSyntheticDefaultImports": true
	},
	"include": ["src"]
}
